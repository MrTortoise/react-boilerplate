{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "react-boilerplate",
  "author": "Lee Cooper",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build:dev": "npm run clean && webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean &&  webpack --config config/webpack.prod.js --progress --profile",
    "server:dev": "npm run build:dev && webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base public/",
    "clean": "rimraf -- public/",
    "test": "mocha test/*.spec.js --compilers js:babel-core/register --recursive"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "history": "^4.5.1",
    "node-sass": "^4.5.0",
    "optimize-js-plugin": "0.0.4",
    "path": "^0.12.7",
    "re-base": "^2.6.0",
    "react": "^15.4.2",
    "react-burger-menu": "^1.10.12",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "rimraf": "^2.6.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.4",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "postcss-loader": "^1.3.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.2",
    "sass-loader": "^6.0.1",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "2.4.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biglee84/react-boilerplate.git"
  },
